    HostName bitbucket.org
    IdentityFile ~/.ssh/deepcast_id_rsa
    User remoteusername

Host gitlab.com
    HostName gitlab.com
    IdentityFile ~/.ssh/gitlab_id_rsa
    User remoteusername

